 203:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_LIMIT_CTL1, 0x00);
 204:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Adjust Bass and Treble levels */
 205:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_TONE_CTL, 0x0F);
 206:Drivers/BSP/Components/cs43l22/cs43l22.c ****   /* Adjust PCM volume level */
 207:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_PCMA_VOL, 0x0A);
 208:Drivers/BSP/Components/cs43l22/cs43l22.c ****   counter += CODEC_IO_Write(DeviceAddr, CS43L22_REG_PCMB_VOL, 0x0A);
