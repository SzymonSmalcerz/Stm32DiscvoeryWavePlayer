 146:Drivers/BSP/Components/cs43l22/cs43l22.c ****   switch (OutputDevice)
 147:Drivers/BSP/Components/cs43l22/cs43l22.c ****   {
 148:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_SPEAKER:
 149:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xFA;
 150:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 151:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 152:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_HEADPHONE:
 153:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xAF;
 154:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 155:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 156:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_BOTH:
 157:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0xAA;
 158:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;
 159:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 160:Drivers/BSP/Components/cs43l22/cs43l22.c ****   case OUTPUT_DEVICE_AUTO:
 161:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0x05;
 162:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;    
 163:Drivers/BSP/Components/cs43l22/cs43l22.c ****     
 164:Drivers/BSP/Components/cs43l22/cs43l22.c ****   default:
 165:Drivers/BSP/Components/cs43l22/cs43l22.c ****     OutputDev = 0x05;
 166:Drivers/BSP/Components/cs43l22/cs43l22.c ****     break;    
 167:Drivers/BSP/Components/cs43l22/cs43l22.c ****   }
